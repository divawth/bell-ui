<div
class="${prefix}space
{{#if vertical}} ${prefix}space-vertical
{{else}} ${prefix}space-horizontal
{{/if}}

{{#if align}} ${prefix}space-align-{{align}}{{/if}}
{{#if autoWrap}} ${prefix}space-auto-wrap{{/if}}
{{#if block}} ${prefix}space-block{{/if}}
{{#if !supportFlexGap}} ${prefix}space-legacy{{/if}}
{{#if className}} {{className}}{{/if}}"

{{#if inlineStyle}}
  style="{{inlineStyle}}"
{{/if}}
>
  <slot />
</div>