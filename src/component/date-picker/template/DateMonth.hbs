{{#if type === RAW_TYPE_MONTH}}
  <div
  class="${prefix}date-picker-month ${prefix}date-picker-panel
  {{#if className}} {{className}}{{/if}}"

  {{#if style}}
    style="{{style}}"
  {{/if}}
  >

    <div class="${prefix}date-picker-header">
      <div class="${prefix}date-picker-prev">
        <span class="${prefix}date-picker-header-button" on-click="decrease('year', 10)">
          <Icon
            name="arrow-left-s-fill"
            size="0"
            className="${prefix}date-picker-header-icon"
          />
        </span>

        <span class="${prefix}date-picker-header-button" on-click="decrease('year', 1)">
          <Icon
            name="arrow-left-s-line"
            size="0"
            className="${prefix}date-picker-header-icon"
          />
        </span>
      </div>

      <span
        {{#if canPickYear}}
          class="${prefix}date-picker-header-link"
          on-click="set('type', RAW_TYPE_YEAR)"
        {{/if}}
      >
        <span class="${prefix}date-picker-header-year">
          {{year}}
        </span>
        å¹´
      </span>

      <div class="${prefix}date-picker-next">
        <span class="${prefix}date-picker-header-button" on-click="increase('year', 1)">
          <Icon
            name="arrow-right-s-line"
            size="0"
            className="${prefix}date-picker-header-icon"
          />
        </span>

        <span class="${prefix}date-picker-header-button" on-click="increase('year', 10)">
          <Icon
            name="arrow-right-s-fill"
            size="0"
            className="${prefix}date-picker-header-icon"
          />
        </span>
      </div>
    </div>

    <div class="${prefix}date-picker-body">
      {{#each datasource}}
        <div class="${prefix}date-picker-row">
          {{#each this}}
            <div
              class="${prefix}date-picker-col ${prefix}date-picker-col-enabled
              {{#if isChecked(this)}} ${prefix}date-picker-col-checked
              {{/if}}"

              on-click="click(this)"
            >
              <div class="${prefix}date-picker-text">
                {{this.text}}
              </div>
            </div>
          {{/each}}
        </div>
      {{/each}}
    </div>

  </div>
{{else if type === RAW_TYPE_YEAR}}
  <DateYear
    checkedYear="{{year}}"
  />
{{/if}}